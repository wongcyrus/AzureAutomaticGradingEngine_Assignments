    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET: Run AzureProjectTest",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build AzureProjectTest",
            "program": "${workspaceFolder}/AzureProjectTest/bin/Debug/net8.0/AzureProjectTest.dll",
            "args": [
                // args are optional; Main.cs currently uses hardcoded paths
                "${workspaceFolder}/testing/sp.json",
                "${workspaceFolder}/testing",
                "trace",
                ""
            ],
            "cwd": "${workspaceFolder}/AzureProjectTest",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "justMyCode": true
        }

    ]
}